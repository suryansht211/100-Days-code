class Solution {
public:
    vector<vector<int>> transpose(vector<vector<int>>& matrix) {
       vector<vector<int>> ans;
        for (int i=0; i<matrix[0].size(); i++){ //no. of cols
            vector<int> t;
            for (int j= 0; j<matrix.size(); j++){ // no. of rows
                t.push_back(matrix[j][i]);
            }
            ans.push_back(t);
        }
        
        return ans;
    }
};
